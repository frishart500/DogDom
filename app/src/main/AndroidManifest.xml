<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com_dog.dog.dog_java.dogdom">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DogDom">
        <activity
            android:name=".ProfileStructure.ChangeDataActivity"
            android:exported="false" />
        <activity
            android:name=".ui.Dashboard.DataActivity"
            android:exported="false" />
        <activity
            android:name=".Auth.RegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".ui.Dashboard.Main2Activity"
            android:exported="false"
            android:label="@string/title_activity_main2"
            android:theme="@style/Theme.DogDom.NoActionBar" />
        <activity
            android:name=".Auth.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Onboarding.OnboardingMainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>